{
  "name": "louper-v3",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "migrate": "bun run migrate:validate && bun run migrate:backup && bun run migrate:upgrade-core && bun run migrate:svelte && bun run migrate:update-deps && bun run migrate:regenerate-components && bun run migrate:test",
    "migrate:validate": "bun run scripts/validate-migration.js",
    "migrate:backup": "rsync -a --exclude='.backup' --exclude='node_modules' --exclude='.git' ./ .backup/pre-migration-$(date +%Y%m%d-%H%M%S)/",
    "migrate:upgrade-core": "bun install svelte@^5.43.3 @sveltejs/kit@^2.48.4 @sveltejs/vite-plugin-svelte@^6.2.1 vite@^5.4.21 -D",
    "migrate:svelte": "bun x sv migrate svelte-5",
    "migrate:update-deps": "bun install bits-ui@^2.14.2 @lucide/svelte@latest tailwindcss-animate -D && bun remove cmdk-sv lucide-svelte radix-icons-svelte",
    "migrate:regenerate-components": "bun x shadcn-svelte@latest add --all --overwrite",
    "migrate:test": "bun run check && bun --bun run build",
    "migrate:rollback": "echo 'Rolling back migration...' && rm -rf node_modules bun.lockb && rsync -a .backup/pre-migration-latest/* . && bun install"
  },
  "devDependencies": {
    "@exodus/schemasafe": "^1.3.0",
    "@sinclair/typebox": "^0.34.41",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.48.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tanstack/table-core": "^8.21.3",
    "@typeschema/class-validator": "^0.3.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vinejs/vine": "^4.1.0",
    "arktype": "^2.1.25",
    "autoprefixer": "^10.4.21",
    "bun-types": "^1.3.1",
    "class-validator": "^0.14.2",
    "drizzle-kit": "0.31.6",
    "embla-carousel-svelte": "8.6.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.0",
    "formsnap": "2.0.1",
    "joi": "^18.0.1",
    "mode-watcher": "^1.1.0",
    "paneforge": "^1.0.2",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "superstruct": "^2.0.2",
    "svelte": "^5.43.4",
    "svelte-adapter-bun": "^1.0.1",
    "svelte-check": "^4.3.3",
    "svelte-sonner": "^1.0.5",
    "sveltekit-superforms": "^2.28.1",
    "tailwindcss": "^3",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "valibot": "^1.1.0",
    "vaul-svelte": "1.0.0-next.7",
    "vite": "^7.2.1",
    "vite-plugin-node-polyfills": "^0.24.0",
    "yup": "^1.7.1",
    "zod": "^4.1.12"
  },
  "type": "module",
  "dependencies": {
    "@lucide/svelte": "^0.552.0",
    "@safe-global/safe-apps-sdk": "^9.1.0",
    "@wagmi/connectors": "^6.1.3",
    "@wagmi/core": "^2.22.1",
    "@web3modal/wagmi": "^5.1.11",
    "bits-ui": "^2.14.2",
    "clsx": "^2.1.1",
    "consola": "^3.4.2",
    "drizzle-orm": "^0.44.7",
    "svelte-french-toast": "^1.2.0",
    "svelte-tags-input": "^6.0.2",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "viem": "^2.38.6"
  },
  "trustedDependencies": [
    "@sveltejs/kit",
    "svelte-preprocess",
    "utf-8-validate"
  ]
}
