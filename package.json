{
  "name": "louper-v3",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "migrate": "bun run migrate:validate && bun run migrate:backup && bun run migrate:upgrade-core && bun run migrate:svelte && bun run migrate:update-deps && bun run migrate:regenerate-components && bun run migrate:test",
    "migrate:validate": "bun run scripts/validate-migration.js",
    "migrate:backup": "rsync -a --exclude='.backup' --exclude='node_modules' --exclude='.git' ./ .backup/pre-migration-$(date +%Y%m%d-%H%M%S)/",
    "migrate:upgrade-core": "bun install svelte@^5.43.3 @sveltejs/kit@^2.48.4 @sveltejs/vite-plugin-svelte@^6.2.1 vite@^5.4.21 -D",
    "migrate:svelte": "bun x sv migrate svelte-5",
    "migrate:update-deps": "bun install bits-ui@^2.14.2 @lucide/svelte@latest tailwindcss-animate -D && bun remove cmdk-sv lucide-svelte radix-icons-svelte",
    "migrate:regenerate-components": "bun x shadcn-svelte@latest add --all --overwrite",
    "migrate:test": "bun run check && bun --bun run build",
    "migrate:rollback": "echo 'Rolling back migration...' && rm -rf node_modules bun.lockb && rsync -a .backup/pre-migration-latest/* . && bun install"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.48.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.20",
    "bun-types": "^1.1.27",
    "drizzle-kit": "0.21.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte": "^2.43.0",
    "postcss": "^8.4.45",
    "postcss-load-config": "^4.0.2",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^5.43.3",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^3.8.6",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.21",
    "vite-plugin-node-polyfills": "^0.21.0"
  },
  "type": "module",
  "dependencies": {
    "@lucide/svelte": "^0.552.0",
    "@safe-global/safe-apps-sdk": "^8.1.0",
    "@wagmi/connectors": "^4.3.10",
    "@wagmi/core": "^2.13.4",
    "@web3modal/wagmi": "^4.2.3",
    "bits-ui": "^2.14.2",
    "clsx": "^2.1.1",
    "consola": "^3.2.3",
    "drizzle-orm": "^0.29.5",
    "svelte-french-toast": "^1.2.0",
    "svelte-tags-input": "^5.0.0",
    "tailwind-merge": "^1.14.0",
    "tailwind-variants": "^0.1.20",
    "viem": "^2.31.4"
  },
  "trustedDependencies": [
    "@sveltejs/kit",
    "svelte-preprocess",
    "utf-8-validate"
  ]
}
